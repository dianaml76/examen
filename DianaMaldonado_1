horasOr = prompt("Ingrese horas laborales");
horasEx = prompt("Ingrese horas extra");

sueldo1 = horasOr * 265.00;
sueldo2 = horasEx * 350.00;

alert("El total a pagar por horas laborales normal " + horasOr + " es de " + sueldo1);
alert("El total a pagar por horas extra " + horasEx + " es de " + sueldo2);
